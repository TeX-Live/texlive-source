  int i;
  (void)argc; (void)argv;
  if (p==0) return 1;
  if (getc(p)!='K' || getc(p)!='o' || getc(p)!=' ') return 2;
  if (getc(p)!='r' || getc(p)!='t') return 3;
  if (getc(p)!='f'-'a'+'0') return 4;
  if (getc(p)!='\n') return 5;
