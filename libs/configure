  esac`
  if test -n "$kpse_read"; then
    kpse_mung=`echo $kpse_inc | sed 's,/,%,g'`
    kpse_substs="$kpse_substs -e '/^kpse_include $kpse_mung$/r $kpse_read'"
  fi
